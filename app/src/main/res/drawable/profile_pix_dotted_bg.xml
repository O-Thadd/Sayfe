<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:height="100dp"
        android:width="100dp">
        <shape android:shape="oval">
            <stroke
                android:color="@color/blue_color"
                android:dashWidth="6dp"
                android:dashGap="6dp"
                android:width="1dp"/>
            <size
                android:width="10dp"
                android:height="10dp"/>
        </shape>
    </item>


</layer-list>